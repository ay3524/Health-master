<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mayank.healthcare">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/launcherlogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:taskAffinity="com.mayank.healthcare.MainActivity">
        </activity>

        <activity android:name=".Main2Activity" />
        <activity android:name=".Register" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SymptomsNeedActivity" />
        <activity android:name=".PredictorActivity" />
        <activity android:name=".medicine" />
        <activity android:name=".medicinedata" />
        <activity android:name=".Conditions" />
        <activity android:name=".ConditionDetailActivity" />
    </application>

</manifest>